<Window x:Class="Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client"
        mc:Ignorable="d"
        Title="MainWindow" MinHeight="250" MinWidth="400">
    <Grid>
        <StackPanel>
            <ComboBox Name="cBox_Methods" Margin="15" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="256" FontSize="20" SelectionChanged="cBox_Methods_SelectionChanged"></ComboBox>
            <TextBox x:Name="tBox" IsEnabled="False" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="20" Width="256"/>
            <Button Content="Request" Margin="15" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="20" Width="256" Height="45" Click="Button_Click"/>
        </StackPanel>
    </Grid>
</Window>
